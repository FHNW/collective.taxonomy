<html xmlns="http://www.w3.org/1999/xhtml"
     xmlns:tal="http://xml.zope.org/namespaces/tal"
     tal:omit-tag=""> 
<span class="option"
      tal:repeat="tmpitem view/items">
<div style="float: left; width: 250px; padding-top: 10px;">
<tal:block tal:define="item python:view.items[tmpitem]">
  <input type="checkbox" id="" name="" class="" alt="" title=""
         tabindex="" disabled="" readonly="" accesskey="" value=""
         checked="checked"
         tal:condition="item/checked"
         tal:attributes="id item/id;
                         name item/name;
                         class view/klass;
                         value item/value;
                         style view/style;
                         title view/title;
                         lang view/lang;
                         disabled view/disabled;
                         tabindex view/tabindex;
                         readonly view/readonly;
                         alt view/alt;
                         accesskey view/accesskey;"
  /><input id="" name="" class="" alt="" title="" tabindex=""
           disabled="" readonly="" accesskey="" value=""
           type="checkbox" 
         tal:condition="not:item/checked"
         tal:attributes="id item/id;
                         name item/name;
                         class view/klass;
                         value item/value;
                         style view/style;
                         title view/title;
                         lang view/lang;
                         disabled view/disabled;
                         tabindex view/tabindex;
                         readonly view/readonly;
                         alt view/alt;
                         accesskey view/accesskey;" />
  <label for=""
         tal:attributes="for item/id">
    <span class="label" tal:content="item/label">Label</span><br/>
  </label>
  <tal:block repeat="subitem item/children">
     <div style="padding-left: 15px">
		  <input type="checkbox" id="" name="" class="" alt="" title=""
		         tabindex="" disabled="" readonly="" accesskey="" value=""
		         checked="checked"
		         tal:condition="subitem/checked"
		         tal:attributes="id subitem/id;
		                         name subitem/name;
		                         class view/klass;
		                         value subitem/value;
		                         style view/style;
		                         title view/title;
		                         lang view/lang;
		                         disabled view/disabled;
		                         tabindex view/tabindex;
		                         readonly view/readonly;
		                         alt view/alt;
		                         accesskey view/accesskey;"
		  /><input id="" name="" class="" alt="" title="" tabindex=""
		           disabled="" readonly="" accesskey="" value=""
		           type="checkbox"
		         tal:condition="not:subitem/checked"
		         tal:attributes="id subitem/id;
		                         name subitem/name;
		                         class view/klass;
		                         value subitem/value;
		                         style view/style;
		                         title view/title;
		                         lang view/lang;
		                         disabled view/disabled;
		                         tabindex view/tabindex;
		                         readonly view/readonly;
		                         alt view/alt;
		                         accesskey view/accesskey;" />
		 
		    <span tal:content="subitem/label">Label</span><br/>
     </div>
  </tal:block>
</tal:block>
</div>
<div style="clear: left" tal:condition="python:repeat['tmpitem'].index % 2 == 1"></div>
</span>
<input name="field-empty-marker" type="hidden" value="1"
       tal:attributes="name string:${view/name}-empty-marker" />
</html>
